{
  "name": "serverless-jest-tools",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "transpile": "babel . --extensions .ts --no-copy-ignored",
    "package": "rm -rf dist && yarn package:cjs && yarn package:types",
    "watch": "rm -rf dist && concurrently 'yarn:package:* --watch'",
    "package:cjs": "NODE_ENV=cjs yarn transpile --out-dir dist/cjs",
    "package:types": "tsc",
    "test:all": "yarn test:linter && yarn test:type && yarn test:unit",
    "test:type": "tsc --noEmit --emitDeclarationOnly false",
    "test:unit": "echo 'no tests'",
    "linter-base-config": "eslint --ext=js,ts",
    "test:linter": "yarn linter-base-config .",
    "lint:fix": "yarn linter-base-config --fix",
    "lint:fix-all": "yarn lint:fix .",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-sdk": "^2.773.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "clinter": "^1.15.0",
    "concurrently": "^6.0.0",
    "danger": "^10.5.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^26.6.0",
    "lerna": "^3.22.1",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.20",
    "npm-audit-resolver": "^2.2.1",
    "prettier": "^2.3.2",
    "syncpack": "^5.6.10",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "^14.16.1",
    "yarn": "^1.22.5"
  }
}
